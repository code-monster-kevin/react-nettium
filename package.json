{
  "name": "react-nettium",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-intl": "^2.8.0",
    "react-redux": "^6.0.1",
    "react-router-dom": "^5.0.0",
    "react-scripts": "2.1.8",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prettier": "prettier --print-width 100 --single-quote --write \"src/**/*.{js,jsx,json,css}\"",
    "extract:messages": "react-intl-cra \"src/**/*.{js,jsx}\" -o \"src/i18n/messages/messages.json\"",
    "manage:translations": "node ./translationRunner.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "prettier": "^1.16.4",
    "react-intl-cra": "^0.3.4",
    "react-intl-translations-manager": "^5.0.3"
  }
}
